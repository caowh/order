<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cwh.order.producer.dao.FoodDeleteDao">

    <insert id="insert" parameterType="cwh.order.producer.model.FoodDelete">
      INSERT INTO FOOD_DELETE(id,f_name,description,price,classify_name,picture_url,openid,delete_time) VALUES
      (#{id},#{f_name},#{description},#{price},#{classify_name},#{picture_url},#{openid},#{delete_time});
    </insert>

</mapper>